{
  "name": "genkitx-groq",
  "description": "Firebase Genkit AI framework plugin for Groq APIs.",
  "keywords": [
    "genkit",
    "genkit-plugin",
    "genkit-model",
    "groq",
    "llama3-8b",
    "llama3-70b",
    "mixtral-8x7b",
    "gemma-7b",
    "ai",
    "genai",
    "generative-ai"
  ],
  "version": "0.0.10",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheFireCo/genkit-plugins.git",
    "directory": "plugins/groq"
  },
  "author": "TheFireCo",
  "license": "Apache-2.0",
  "dependencies": {
    "@genkit-ai/ai": "0.5.0-rc.8",
